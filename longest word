def LongestWord(sen)

  # code goes here
  ans = sen.split(' ')
  s = ans[0]
  ans.each do |x|
    if s.length < x.length
      s = x
    end
  end
  
    
    
  return s
         
end
   
# keep this function call here 
# to see how to enter arguments in Ruby scroll down   
LongestWord(STDIN.gets)  


















  
